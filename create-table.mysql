DROP TABLE IF EXISTS game;
CREATE TABLE game (
    id        INT NOT NULL AUTO_INCREMENT,
    name      VARCHAR(32) NOT NULL,
    genre     VARCHAR(32) NOT NULL,
    year      INT NOT NULL,
    publisher VARCHAR(32) NOT NULL,
    PRIMARY KEY (id)
);

